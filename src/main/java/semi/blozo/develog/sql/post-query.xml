<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
 <!-- properties 객체 안에 작성하기 -->
<properties>
 
	<!--  -->
	<entry key="">
		
	</entry>
	
	<!-- 게시글 삽입 + 이미지 정보 삽입 총 3번 -->
	<!-- 다음 게시글 번호 조회 -->
	<entry key="nextPostNo">
		SELECT SEQ_POST.NEXTVAL FROM DUAL
	</entry>
	
	<!-- 게시글 삽입 -->
	<entry key="insertPost">
		INSERT INTO POST 
		VALUES(SEQ_POST.NEXTVAL, ?, ?, DEFAULT, DEFAULT, DEFAULT, DEFAULT,DEFAULT, ?, ?, ?)
	</entry>
	
	<!-- 게시글 이미지 정보 삽입 -->
	<entry key="insertPostImage">
		INSERT INTO POST_IMG 
		VALUES( SEQ_POST_IMG.NEXTVAL, ?, ?, DEFAULT, ?, ?, ?)
	</entry>
	
	<!-- 태그 삽입 -->
	<entry key="insertTag">
		INSERT INTO TAG
		VALUES (SEQ_TAG.NEXTVAL, ?, ?)
	</entry>
	
	
</properties>
